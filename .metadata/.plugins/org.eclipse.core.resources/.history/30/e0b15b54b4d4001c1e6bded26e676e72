package Study;

public class ArrayCopyEx02 {
	public static void main(String[] args) {
		int[] arr = new int[5];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = i + 1;
			System.out.print(arr[i] + "\t");
		}
		System.out.println();
		System.out.println("º¯°æ Àü arr.length : " + arr.length);
	}
}
