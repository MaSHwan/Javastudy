package com.constexx;

class ThisEx {
	String name;
	String tel;
	String jumin;

	public ThisEx() {
		this.name = "Guest";
		this.tel = "010 - 1234 - 5678";
		this.jumin = "123456 - 1234567";
	}

	public ThisEx(String name) {
		this();
		this.name = name;
	}

	public ThisEx(String name, String jumin) {
		this(name);
		this.jumin = jumin;
	}

	public ThisEx(String name, String jumin, String tel) {
		this(name, jumin);
		this.tel = tel;
	}

	public String getName() {
		return name;
	}

	public String getTel() {
		return tel;
	}

	public String getJumin() {
		return jumin;
	}
	
	

}

public class ThisEx04 {
	public static void main(String[] args) {
		ThisEx t = new ThisEx();
		System.out.println("Name : " + t.getName());
		System.out.println("Jumin : " + t.getJumin());
		System.out.println("Tel : " + t.getTel());
		
		
	}
}
