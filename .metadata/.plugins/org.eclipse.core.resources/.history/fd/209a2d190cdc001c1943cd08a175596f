package com.play;

public class Piano extends Instrument {
	public static void main(String[] args) {
		Piano p = new Piano();
		p.play();
		p.volumeup();
		p.volumedown();
	}
		
	

	@Override
	void play() {
		// TODO Auto-generated method stub
		System.out.println("Piano : play() »£√‚µ ");
	}

	@Override
	void volumeup() {
		// TODO Auto-generated method stub
		System.out.println("Piano : volume up() »£√‚µ ");
	}

	@Override
	void volumedown() {
		// TODO Auto-generated method stub
		System.out.println("Piano : volume down() »£√‚µ ");
	}

}
